module "alb" {
  source                         = "./alb"
  subnets                        = ["${module.vpc.public_subnets}"]
  alb_name                       = {{{data.albName}}}
  environment                    = {{{data.environment}}}
  tag_name                       = {{{data.tagName}}}
  alb_security_group_name        = {{{data.albSecurityGroupName}}}
  alb_security_group_description = {{{data.albSecurityGroupDescription}}}
  vpc_id                         = "${module.vpc.vpc_id}"
}